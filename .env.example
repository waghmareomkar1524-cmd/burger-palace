# Twilio Configuration
# Get these values from your Twilio Console: https://console.twilio.com/
REACT_APP_TWILIO_ACCOUNT_SID=your_account_sid_here
REACT_APP_TWILIO_AUTH_TOKEN=your_auth_token_here
REACT_APP_TWILIO_FROM_NUMBER=your_twilio_phone_number_here

# Razorpay Configuration (Optional - for payments)
# Get these from your Razorpay Dashboard: https://dashboard.razorpay.com/app/keys
REACT_APP_RAZORPAY_KEY_ID=your_razorpay_key_id_here
REACT_APP_RAZORPAY_KEY_SECRET=your_razorpay_key_secret_here

# Example:
# REACT_APP_TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# REACT_APP_TWILIO_AUTH_TOKEN=your_auth_token_here
# REACT_APP_TWILIO_FROM_NUMBER=+1234567890
# REACT_APP_RAZORPAY_KEY_ID=rzp_test_xxxxxxxxxxxxxxxx
# REACT_APP_RAZORPAY_KEY_SECRET=your_razorpay_secret_here
